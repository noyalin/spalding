    function modifyAddress(id){
        jQuery.ajax({
            type: 'POST',
            //url: 'https://mp.weixin.qq.com/cgi-bin/login?lang=zh_CN',
            url: '<?php echo Mage::getBaseUrl(Mage_Core_Model_Store::URL_TYPE_WEB); ?>/customer/address/getAddressInfo/',
            data:{
                id:id
            },
            success: function(data){
                var dataObj = eval("("+data+")");
                jQuery( "#dialog" ).dialog( "open" );
                jQuery(".ui-dialog-title").html("修改收货地址");
                jQuery(".new_address #firstname").val(dataObj.firstname);
                jQuery(".new_address #addressid").val(dataObj.entity_id);
                jQuery(".new_address #street_1").val(dataObj.street);
                jQuery(".new_address #postcode").val(dataObj.postcode);
                jQuery(".new_address #telephone").val(dataObj.telephone);
                jQuery(".new_address #city").val(dataObj.city);
                jQuery(".new_address #region").val(dataObj.region_id);
                jQuery(".new_address #district").val(dataObj.district);
                jQuery('.new_address #region_id ').val(dataObj.region_id);

                jQuery.ajax({
                    type: 'POST',
                    url: '<?php echo $this->getUrl('checkout/area/getCityList') ?>',
                    data: {id:dataObj.region_id},
                    success: function(data, textStatus){
                        jQuery('#city_id').html(data);
                        jQuery('.new_address #city_id ').val(dataObj.city_id);
                    },
                    error: function(){
                    }
                });

                jQuery.ajax({
                    type: 'POST',
                    url: '<?php echo $this->getUrl('checkout/area/getDistrictList') ?>',
                    data: {id:dataObj.city_id},
                    success: function(data, textStatus){
                        jQuery('#district_id').html(data);
                        jQuery('.new_address #district_id ').val(dataObj.district_id);
                    },
                    error: function(){
                    }
                });

            },
            error: function(e){}
        });
    }
